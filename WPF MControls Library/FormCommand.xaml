<Window x:Class="WPFMControls.FormCommand"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        DataContext="{Binding RelativeSource={RelativeSource Self}}"
        Style="{DynamicResource SkinWindowStyle}"
        Icon="Resources\Logo.ico"
        Template="{DynamicResource WindowTemplate }"
        Title="FormCommand" xmlns:my="clr-namespace:WPFMControls" Loaded="Window_Loaded" SizeToContent="WidthAndHeight" WindowStartupLocation="CenterOwner" ResizeMode="NoResize">
    <Grid Background="#FF131313" Height="314" Width="360">
        <my:MPropsControl HorizontalAlignment="Center" Margin="0,62,0,0" x:Name="mPropsControl1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Name="textBoxCommand" VerticalAlignment="Top" Width="269" Margin="5,7,0,0" />
        <Button Content="Set" Height="23" HorizontalAlignment="Right" Margin="0,7,5,0" Name="buttonSet" VerticalAlignment="Top" Width="75" Click="buttonSet_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="5,32,0,0" Name="textBoxParams" VerticalAlignment="Top" Width="269" />
    </Grid>
</Window>
